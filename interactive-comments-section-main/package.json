{
	"name": "interactive-comments-section-main",
	"version": "1.0.0",
	"description": "![Design preview for the Interactive comments section coding challenge](./design/desktop-preview.jpg)",
	"main": "index.js",
	"scripts": {
		"watch:sass": "sass sass/main.scss styles/style.css --watch",
		"compile:sass": "sass sass/main.scss style/compiled.css",
		"prefix:css": "postcss style/compiled.css -o style/prefix.css --use autoprefixer",
		"compress:css": "sass style/prefix.css style/styles.css --style=compressed",
		"build:css": "npm-run-all compile:sass prefix:css compress:css"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"autoprefixer": "^10.4.2",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^9.1.0"
	}
}
